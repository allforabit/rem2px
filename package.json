  "homepage": "https://github.com/allforabit/rem2px#readme",
  "devDependencies": {
    "@parcel/config-webextension": "^2.2.1",
    "@types/chrome": "^0.0.177",
    "autoprefixer": "^10.4.2",
    "parcel": "^2.2.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.16"
